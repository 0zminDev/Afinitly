{
	"name": "generate-ports",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "tools/generate-ports/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/tools/generate-ports",
				"tsConfig": "tools/generate-ports/tsconfig.lib.json",
				"packageJson": "tools/generate-ports/package.json",
				"main": "tools/generate-ports/src/index.ts",
				"assets": ["tools/generate-ports/*.md"]
			}
		},
		"generate": {
			"executor": "nx:run-commands",
			"options": {
				"command": "ts-node tools/generate-ports/src/index.ts"
			}
		},
        "purge": {
            "executor": "nx:run-commands",
            "options": {
                "command": "ts-node tools/generate-ports/src/index.ts purge"
            }
        }
	}
}
