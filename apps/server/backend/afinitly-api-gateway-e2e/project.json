{
	"name": "afinitly-api-gateway-e2e",
	"$schema": "../../../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"implicitDependencies": ["afinitly-api-gateway"],
	"targets": {
		"e2e": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
			"options": {
				"jestConfig": "apps/server/backend/afinitly-api-gateway-e2e/jest.config.ts",
				"passWithNoTests": true
			},
			"dependsOn": [
				"afinitly-api-gateway:build",
				"afinitly-api-gateway:serve"
			]
		},
		"docker:backend:api-gateway-e2e:build": {
			"dependsOn": ["build", "prune"]
		}
	}
}
